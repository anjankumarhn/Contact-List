<%= form_for :contact , :url => contacts_path  do |f| %>
<%= f.label 'First name  :' %> <br />
<%= f.text_field :firstname %> <br /> <br />
<%= f.label 'Last name   :' %> <br />
<%= f.text_field :lastname %> <br /> <br />
<%= f.label 'email    :' %> <br />
<%= f.text_field :email %><br /> <br />
<%= f.label 'Phone number:' %> <br />
<%= f.text_field :phonenumber %> <br /> <br />
<br />
<%= f.submit 'Create' %>
<% end %>